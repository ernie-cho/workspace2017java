package baekjoon;

import java.util.Scanner;

public class array_2577 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int A=0, B=0, C=0;
		int result = 0;
		Scanner scan = new Scanner(System.in);
		
		A = scan.nextInt();
		B = scan.nextInt();
		C = scan.nextInt();
		result = A*B*C;
		
		System.out.println(result);
		
	}

}
